digraph "doc_python_target_handlers" {
	graph [
		fontsize = "14"
		fontname = "Times-Roman"
		fontcolor = "black"
    pad = 1
	]
	node [
		fontsize = "15"
		fontname = "Bitstream Vera Sans"
		shape = "record"
		style = "solid"
		color = "grey"
	]
	edge [
		fontsize = "14"
		fontname = "Bitstream Vera Sans"
		style = "dotted"
		weight = "4"
	]

	"cmd:main" -> "cmd:argv" ;

	"txs:session" -> "cmd:main" ;

	"txs:pwd" -> "txs:session" ;
	"txs:ls" -> "txs:session" ;
	"txs:run" -> "txs:session" ;

  "rsr2:workspace" -> "cmd:main" ;
  "rsr2:volume" -> "rsr2:workspace" ;

  "rsr:add" -> "rsr2:volume" ;
  "rsr:status" -> "rsr2:volume" ;
  "rsr:update-volume" -> "rsr2:volume" ;
  "rsr:update-metafiles" -> "rsr2:volume" ;
  "rsr:meta" -> "rsr2:volume" ;


  "lnd:tag" -> "txs:pwd" ;

  edge [ style="normal" ] ;
}
