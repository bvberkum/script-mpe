#!/bin/sh
# Created: 2016-04-03

set -e


main_entry()
{
  local scriptpath=$HOME/bin
  true "${U_S:=/srv/project-local/user-scripts/src/sh/lib}"
  . $U_S/os.lib.sh
  . $scriptpath/os-htd.lib.sh
  rotate_file "$@" || return
}


test "$(basename $0)" = "rotate-file" && {
  main_entry $@ || exit $?
}
